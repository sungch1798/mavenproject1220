<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckLogin1</servlet-name>
        <servlet-class>com.mycompany.mavenproject1.CheckLogin1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckLogin2</servlet-name>
        <servlet-class>com.mycompany.mavenproject1.CheckLogin2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckLogin1</servlet-name>
        <url-pattern>/CheckLogin1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckLogin2</servlet-name>
        <url-pattern>/CheckLogin2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            1
        </session-timeout>
    </session-config>
</web-app>
